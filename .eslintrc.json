{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/vue3-essential", "standard", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    // 双引号
    "quotes": ["error", "always"],
    // 分号
    "semi": ["error", "always"],
    // 结尾逗号
    "comma-dangle": ["error", "never"]
  }
}
